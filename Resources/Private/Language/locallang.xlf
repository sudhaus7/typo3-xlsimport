		<body>
			<trans-unit id="module_name">
				<source>XLS Importer</source>
			</trans-unit>
			<trans-unit id="description">
				<source>This module provides an importer for spreadsheet data for the most in TYPO3 configured tables using a fluid importer.</source>
			</trans-unit>
			<trans-unit id="shortDescription">
				<source>Spreadsheet importer for TYPO3</source>
			</trans-unit>
			<trans-unit id="choosepage">
				<source>Choose page</source>
			</trans-unit>
			<trans-unit id="choosepage_info">
				<source>Before continuing, you have to choose a page on the left.</source>
			</trans-unit>
			<trans-unit id="importinfo">
				<source>Import Excel</source>
			</trans-unit>
			<trans-unit id="importinfo_format">
				<source>You can import from following formats:</source>
			</trans-unit>
			<trans-unit id="tableselect">
				<source>Select a table importing to</source>
			</trans-unit>
			<trans-unit id="fileselect">
				<source>Select a file</source>
			</trans-unit>
			<trans-unit id="upload">
				<source>Upload</source>
			</trans-unit>
			<trans-unit id="prepare">
				<source>Prepare data</source>
			</trans-unit>
			<trans-unit id="prepareinfo">
				<source>Prepare Excel</source>
			</trans-unit>
			<trans-unit id="prepareinfo_text">
				<source>Select the structure for the table</source>
			</trans-unit>
			<trans-unit id="importinfo_text">
				<source>Select the records to import</source>
			</trans-unit>
			<trans-unit id="record">
				<source>Import record</source>
			</trans-unit>
			<trans-unit id="ignore">
				<source>ignore</source>
			</trans-unit>
			<trans-unit id="multiple">
				<source>Select default values for multiple fields</source>
			</trans-unit>
			<trans-unit id="import">
				<source>Import</source>
			</trans-unit>
			<trans-unit id="success">
				<source>Data imported successfully</source>
			</trans-unit>
			<trans-unit id="complete">
				<source>Completed</source>
			</trans-unit>
			<trans-unit id="info_complete">
				<source>Total datasets to import</source>
			</trans-unit>
			<trans-unit id="encoding">
				<source>Only for CSV files: file is CP1252 encoded (e.g. Excel export)</source>
			</trans-unit>
			<trans-unit id="deleteRecord">
				<source>Delete all records according to this table on current page?</source>
			</trans-unit>
			<trans-unit id="overridePassword">
				<source>Override password with random values?</source>
			</trans-unit>

			<trans-unit id="uid">
				<source>Unique ID [uid]</source>
			</trans-unit>

			<trans-unit id="pid">
				<source>Page ID [pid]</source>
			</trans-unit>

			<trans-unit id="allowedTables">
				<source>Allowed tables</source>
			</trans-unit>

			<trans-unit id="warning.fieldlist.empty.header">
				<source>Empty field list</source>
			</trans-unit>
			<trans-unit id="warning.fieldlist.empty.message">
				<source>You didn't choose any fields to map. Please correct in dropdowns below and submit again.</source>
			</trans-unit>

			<trans-unit id="error.file.uploadFailed.header">
				<source>File upload failed</source>
			</trans-unit>
			<trans-unit id="error.file.uploadFailed.message">
				<source>Something went wrong during the upload. Please try again.</source>
			</trans-unit>
			<trans-unit id="error.jsonFile.header">
				<source>Temporarily saved file not found</source>
			</trans-unit>
			<trans-unit id="error.jsonFile.message">
				<source>The temporarily saved file was not found. Please try again with upload your data.</source>
			</trans-unit>

			<trans-unit id="uploadForm.no_allowed_tables_available_title">
				<source>Incomplete configuration</source>
			</trans-unit>
			<trans-unit id="uploadForm.no_allowed_tables_available_message">
				<source>No allowed tables for import configured. Refer documentation for required PageTSConfig configuration.</source>
			</trans-unit>
